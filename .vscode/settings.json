{
	"typescript.preferences.importModuleSpecifier": "non-relative",
	"typescript.preferences.importModuleSpecifierEnding": "js",
	"javascript.preferences.quoteStyle": "single",
	"typescript.preferences.quoteStyle": "single",
	"biome.enabled": true,
	"files.exclude": {
		"**/*.js": {
			"when": "$(basename).ts"
		},
		"**/*.js.map": {
			"when": "$(basename)"
		},
		"**/*.d.ts": {
			"when": "$(basename).ts"
		},
		"**/*.min.js": true
	}
}
